-- Scripts:

local function CYHIAG_fake_script() -- TopBar.LocalScript 
	local script = Instance.new('LocalScript', TopBar)

	local frame = script.Parent.Parent
	frame.Active = true
	frame.Draggable = true
end
coroutine.wrap(CYHIAG_fake_script)()
local function QLRHINJ_fake_script() -- Execute.LocalScript 
	local script = Instance.new('LocalScript', Execute)

	local remote = game.ReplicatedStorage:WaitForChild("RemoteEvent")
	script.Parent.MouseButton1Click:Connect(function()
		local code = script.Parent.Parent.Code
		remote:FireServer(code.Text)
	end)
end
coroutine.wrap(QLRHINJ_fake_script)()
local function BGLZV_fake_script() -- Clear.LocalScript 
	local script = Instance.new('LocalScript', Clear)

	local txt = script.Parent.Parent.Code
	script.Parent.MouseButton1Click:Connect(function()
		txt.Text = ""
	end)
end
coroutine.wrap(BGLZV_fake_script)()
local function PLXMS_fake_script() -- Scanner_2.LocalScript 
	local script = Instance.new('LocalScript', Scanner_2)

	local mainframe = script.Parent.Parent.Parent.MainFrame
	local remote = game.ReplicatedStorage:WaitForChild("RemoteEvent")
	local frame = script.Parent.Parent
	script.Parent.MouseButton1Click:Connect(function()
		if remote then
			mainframe.Visible = true
			frame.Visible = false
		end
	end)
end
coroutine.wrap(PLXMS_fake_script)()
local function OTGL_fake_script() -- Scanner.LocalScript 
	local script = Instance.new('LocalScript', Scanner)

	script.Parent.Active = true
	script.Parent.Draggable = true
end
coroutine.wrap(OTGL_fake_script)()
